{
  "name": "rumahsubsidi-id",
  "version": "1.0.0",
  "description": "Platform marketplace resmi untuk rumah subsidi di Indonesia",
  "main": "index.html",
  "scripts": {
    "config": "node generate-config.js",
    "config:dev": "node generate-config.js dev",
    "config:prod": "node generate-config.js prod",
    "config:prod.local": "node generate-config.js prod.local",
    "start": "bash start-all.sh",
    "start:dev": "bash start-dev.sh",
    "start:prod": "bash start-prod.sh",
    "start:prod.local": "bash start-prod-local.sh",
    "start:web": "bash start-web.sh",
    "start:api": "bash start-api.sh",
    "stop": "bash stop-all.sh",
    "status": "bash status.sh",
    "dev": "npx live-server --port=8080",
    "proxy": "node proxy-server.js",
    "test": "echo 'Run tests here'",
    "lighthouse": "lighthouse http://localhost:8080 --view",
    "build": "node generate-config.js && echo 'Build completed'",
    "build:dev": "node generate-config.js dev && echo 'Dev build completed'",
    "build:prod": "node generate-config.js prod && echo 'Production build completed'",
    "build:prod.local": "node generate-config.js prod.local && echo 'Production (local testing) build completed'"
  },
  "keywords": [
    "rumah-subsidi",
    "real-estate",
    "property",
    "indonesia",
    "marketplace"
  ],
  "author": "RumahSubsidi.id Team",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "js-yaml": "^4.1.0"
  }
}
